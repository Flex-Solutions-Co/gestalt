{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gestalt/hero-banner-section",
  "title": "Hero Banner Section",
  "icon": "format-image",
  "description": "Displaying page/post banner image.",
  "category": "gestalt-blocks",
  "keywords": [
    "herosection",
    "image"
  ],
  "version": "1.0.0",
  "supports": {
    "align": true
  },
  "textdomain": "gestalt",
  "attributes": {
    "desktopMediaId": {
      "type": "string"
    },
    "desktopMediaSrc": {
      "type": "string"
    },
    "mobileMediaId": {
      "type": "string"
    },
    "mobileMediaSrc": {
      "type": "string"
    },
    "cover": {
      "type": "string",
      "default": ""
    }
  },
  "example": {
    "attributes": {
      "preview": true
    }
  }
}