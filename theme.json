{
	"$schema": "https://schemas.wp.org/wp/6.3/theme.json",
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "page-with-white-header",
			"postTypes": [
				"page"
			],
			"title": "Page with white header"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#000000",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#ffffff",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#878787",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "#cdd0cf",
					"name": "Light Gray",
					"slug": "light-gray"
				},
				{
					"color": "#fde8e5",
					"name": "Light Pink",
					"slug": "light-pink"
				}
			]
		},
		"custom": {
			"typography": {
				"line-height": {
					"normal": 1.1
				}
			}
		},
		"layout": {
			"contentSize": "1640px",
			"wideSize": "1920px"
		},
		"spacing": {
			"padding": true,
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Helvetica Neue LT",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/helvetica_neue_lt_std_75_bold-webfont.woff2"
							]
						},
						{
							"fontFamily": "Helvetica Neue LT",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/helvetica_neue_lt_std_55_roman-webfont.woff2"
							]
						}
					],
					"fontFamily": "\"Helvetica Neue LT\", serif",
					"name": "Helvetica Neue LT",
					"slug": "helveticaneuelt"
				}
			],
			"fontSizes": [
				{
					"size": "1rem",
					"slug": "small"
				},
				{
					"size": "clamp(1rem, 0.9697rem + 0.1294vw, 1.125rem)",
					"slug": "medium"
				},
				{
					"size": "clamp(1.125rem, 1.0947rem + 0.1294vw, 1.25rem)",
					"slug": "large"
				},
				{
					"size": "clamp(1.375rem, 1.3143rem + 0.2589vw, 1.625rem)",
					"slug": "xl-large"
				},
				{
					"size": "clamp(1.75rem, 1.4163rem + 1.4239vw, 3.125rem)",
					"slug": "huge"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--helveticaneuelt)",
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--helveticaneuelt)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--helveticaneuelt)",
					"fontSize": "var(--wp--preset--font-size--xl-large)",
					"fontStyle": "italic",
					"fontWeight": "normal",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--secondary)",
			"text": "var(--wp--preset--color--primary)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--helveticaneuelt)",
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"letterSpacing" : "-0.1rem"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--helveticaneuelt)",
					"fontSize": "var(--wp--preset--font-size--xl-large)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--helveticaneuelt)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--helveticaneuelt)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--helveticaneuelt)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--helveticaneuelt)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--helveticaneuelt)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "400",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "header",
			"name": "white-header",
			"title": "White Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}